    "cli:category-manager": "npm run build && node dist/interfaces/category-manager.js",
    "cli:admin-dashboard": "npm run build && node dist/interfaces/admin-dashboard.js"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "better-sqlite3": "^11.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.12.7",
    "@types/prompts": "^2.4.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }

{
  "name": "online-wiki-database",
  "version": "1.0.0",
  "description": "SQLite-powered online wiki database project for CSC4103.",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "seed": "npm run build && node dist/database/seed-data.js",
    "seed:dev": "ts-node database/seed-data.ts",
    "run:queries": "npm run build && node dist/queries/run-queries.js",
    "cli:user-management": "npm run build && node dist/interfaces/user-management.js",
    "cli:article-editor": "npm run build && node dist/interfaces/article-editor.js",
